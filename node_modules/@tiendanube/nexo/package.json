{
  "name": "@tiendanube/nexo",
  "version": "1.2.1",
  "description": "Nexo provides tools for communication between an external application and the Nuvemshop administrator.",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "yarn clean && webpack && yarn build:fix-type-defs",
    "build:fix-type-defs": "node ./.scripts/fix-type-defs-imports",
    "publish:release": "yarn bump:check && yarn bump:stable && yarn build && npm publish --tolerate-republish --access public",
    "publish:next": "yarn bump:check && yarn bump:next && npm run build && npm publish --tolerate-republish --access public --tag next",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "clean": "rm -rf dist",
    "bump:stable": "yarn version apply --all",
    "bump:next": "yarn version apply --all --prerelease",
    "bump:check": "yarn version check"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TiendaNube/admin-nexo.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TiendaNube/admin-nexo/issues"
  },
  "homepage": "https://tiendanube.github.io/devhub-apps/docs/developer-tools/nexo/#create-a-nexo-instance",
  "peerDependencies": {
    "react": "^18"
  },
  "devDependencies": {
    "@types/node": "^20.5.7",
    "@types/react": "^18",
    "@typescript-eslint/eslint-plugin": "5.36.1",
    "@typescript-eslint/parser": "5.36.1",
    "dotenv": "^16.3.1",
    "dts-bundle-generator": "^8.0.1",
    "eslint": "8.23.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.1",
    "parcel": "^2.9.3",
    "prettier": "2.7.1",
    "react": "^18.2.0",
    "replace-in-files": "^3.0.0",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "typescript": "4.9.5",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-merge": "^5.9.0",
    "webpack-shell-plugin-next": "^2.3.1"
  },
  "engines": {
    "node": ">=16.x"
  }
}
